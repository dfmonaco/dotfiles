{
  "mcpServers": {
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "command": "npx"
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx"
    },
    "brave-search": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "disabled": true,
      "command": "npx",
      "env": {
        "BRAVE_API_KEY": "YOUR_API_KEY_HERE"
      }
    }
  },
  "nativeMCPServers": {
    "neovim": {
      "disabled_prompts": [
        "parrot"
      ]
    }
  }
}
